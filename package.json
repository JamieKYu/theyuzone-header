{
  "name": "@jamiekyu/website-header",
  "version": "1.0.13",
  "description": "Shared header component for Next.js applications",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist",
    "dist/header.css"
  ],
  "scripts": {
    "build:css": "tailwindcss -i ./src/styles.css -o ./dist/header.css --minify",
    "build:ts": "tsc",
    "build": "npm run build:css && npm run build:ts",
    "dev": "tsc --watch",
    "prepublishOnly": "npm run build",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "next": ">=13.0.0",
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-interactions": "^8.6.14",
    "@storybook/addon-onboarding": "^8.6.14",
    "@storybook/blocks": "^8.6.14",
    "@storybook/react-webpack5": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "autoprefixer": "^10.4.21",
    "next": "15.5.2",
    "postcss": "^8.5.6",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "storybook": "^8.6.14",
    "tailwindcss": "^3.4.17",
    "typescript": "^5",
    "vite": "^7.1.5",
    "ts-loader": "^9.5.1"
  },
  "keywords": [
    "react",
    "nextjs",
    "header",
    "component",
    "theyuzone"
  ],
  "author": "jamiekyu",
